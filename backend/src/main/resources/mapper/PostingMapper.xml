<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.talang.wabackend.mapper.PostingMapper">
    <delete id="deletePosting">
        delete
        from sd_postings
        where id in
        <foreach collection="posting" item="id" separator="," open="(" close=")">
            #{id}
        </foreach>

    </delete>
</mapper>